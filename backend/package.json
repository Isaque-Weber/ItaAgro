{
  "name": "backend",
  "version": "0.1.0",
  "description": "Backend for the Itaagro MVP",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev.b": "npx tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "tsx src/seed.ts",
    "create-plans": "tsx scripts/create-subscription-plans.ts",
    "update-assistant-tools": "tsx src/scripts/updateAssistantTools.ts",
    "migration:generate:custom": "tsx node_modules/typeorm/cli.js migration:generate --dataSource src/services/typeorm/data-source.ts",
    "migration:generate": "tsx node_modules/typeorm/cli.js migration:generate --dataSource src/services/typeorm/data-source.ts -n InitSchema",
    "migration:run": "tsx node_modules/typeorm/cli.js migration:run      --dataSource src/services/typeorm/data-source.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "keywords": [],
  "author": "ItaAgro",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/session": "^11.1.0",
    "@googleapis/gmail": "^13.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.3.2",
    "form-data": "^4.0.3",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.7.0",
    "node-cron": "^4.2.1",
    "openai": "^4.103.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.0",
    "pump": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.1.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.3",
    "@types/nodemailer": "^6.4.17",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.2",
    "@types/pump": "^1.1.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typeorm": "^0.3.24"
  }
}
